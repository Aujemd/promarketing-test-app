{
  "name": "promarketing-test-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "npm run tailwind:dev && storybook dev -p 6006 -s public",
    "build-storybook": "storybook build",
    "prepare": "husky install",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "tailwind:build": "rimraf ./src/index.css && NODE_ENV=production tailwindcss -i ./tailwind/tailwind.css -o ./src/index.css",
    "tailwind:dev": "rimraf ./src/index.css && tailwindcss -i ./tailwind/tailwind.css -s -o ./src/index.css"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.17.8",
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@storybook/addon-essentials": "^7.6.7",
    "@storybook/addon-interactions": "^7.6.7",
    "@storybook/addon-links": "^7.6.7",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/blocks": "^7.6.7",
    "@storybook/nextjs": "^7.6.7",
    "@storybook/react": "^7.6.7",
    "@storybook/test": "^7.6.7",
    "@tanstack/eslint-plugin-query": "^5.17.7",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "storybook": "^7.6.7",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
